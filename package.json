{
  "name": "axis-cambrils-fisioterapia",
  "type": "module",
  "version": "1.0.0",
  "description": "Landing page multiidioma para Axis Cambrils - Centro de Fisioterapia",
  "author": "Axis Cambrils",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xaviibaez/fisio-landing.git"
  },
  "keywords": [
    "fisioterapia",
    "astro",
    "tailwindcss",
    "i18n",
    "landing-page"
  ],
  "scripts": {
    "dev": "astro dev",
    "dev:cms": "concurrently \"decap-server\" \"astro dev\" --names \"CMS,ASTRO\" --prefix-colors \"cyan,green\"",
    "cms:server": "npx decap-server",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/netlify": "^6.6.4",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^5.7.0",
    "decap-cms-app": "^3.10.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "concurrently": "^9.2.1",
    "decap-server": "^3.5.0"
  }
}
